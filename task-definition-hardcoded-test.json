{
    "family": "web-demo-backend",
    "taskRoleArn": "arn:aws:iam::360066926992:role/web-demo-ecs-task-role",
    "executionRoleArn": "arn:aws:iam::360066926992:role/web-demo-ecs-task-execution-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
            "name": "backend",
            "image": "360066926992.dkr.ecr.eu-central-1.amazonaws.com/web-demo-web:backend-hardcoded-test",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "DB_NAME",
                    "value": "visionstudio"
                },
                {
                    "name": "NODE_ENV",
                    "value": "production"
                },
                {
                    "name": "DB_HOST",
                    "value": "web-demo-mysql.cpg4cm66mp51.eu-central-1.rds.amazonaws.com"
                },
                {
                    "name": "DB_USER",
                    "value": "admin"
                },
                {
                    "name": "DB_PASSWORD",
                    "value": "xyz123"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/web-demo-backend",
                    "awslogs-region": "eu-central-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}
